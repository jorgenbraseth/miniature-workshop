# Environment variables for Miniature Workshop frontend
# Copy this file to .env.local and update with your values

# Backend API URL (use deployed URL or local development)
VITE_API_BASE_URL=https://your-api-url.execute-api.eu-west-1.amazonaws.com

# Google OAuth Client ID (get from Google Cloud Console)
VITE_GOOGLE_CLIENT_ID=your-google-client-id-here.apps.googleusercontent.com

# S3 Images Bucket (for production)
VITE_IMAGES_BUCKET=miniature-workshop-backend-prod-images 